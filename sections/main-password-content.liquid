{{ 'banner-text.css' | asset_url | stylesheet_tag }}

{% liquid
  assign st = section.settings
  assign container = st.container
  assign enable_banner = st.enable_banner
  assign banner_img = st.banner_img
  assign banner_position = st.banner_position
  assign banner_width = st.banner_width
  assign text_position = st.text_position
  assign text_horizontal_position = st.text_horizontal_position
  assign content_width = st.content_width

  if enable_banner
    assign show_banner = ' show-banner'
  endif
  assign banner_position_class = ''
  if banner_position == 'position-right'
    assign banner_position_class = ' flex-row-reverse'
  endif
%}

<div class="bls-main-password">
  <div class="{{ container }} section-full">
    <div
      class="row flex-nowrap {{ banner_position_class }} bls__banner-text--grid"
      style="--page-banner-width: {{ banner_width }}%;"
    >
      {% if enable_banner %}
        <div class="bls__password-banner relative d-none d-xl-block">
          {% if banner_img != blank %}
            <img
              loading="lazy"
              src="{{ banner_img | image_url }}"
              alt="{{ banner_img.alt }}"
              width="{{ banner_img.width }}"
              height="{{ banner_img.height }}"
            >
          {% else %}
            {%- render 'responsive-image' | cls: "h-full" -%}
          {% endif %}
        </div>
      {% endif %}
      <div class="bls__banner-text--content {{ show_banner }} {{ st.text_align }} content-position-{{ text_position }}">
        <h2 class="password-modal__content-heading mb-0 d-flex align-items-center" id="DialogHeading">
          <svg
            width="14"
            height="15"
            viewBox="0 0 14 15"
            fill="none"
            xmlns="http://www.w3.org/2000/svg"
          >
