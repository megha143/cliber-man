          "label": "t:sections.main-password-content.blocks.heading.settings.heading_fw.label",
          "min": 100,
          "max": 900,
          "step": 100,
          "default": 500
        },
        {
          "type": "select",
          "id": "heading_tf",
          "label": "t:sections.main-password-content.blocks.heading.settings.heading_tf.label",
          "options": [
            {
